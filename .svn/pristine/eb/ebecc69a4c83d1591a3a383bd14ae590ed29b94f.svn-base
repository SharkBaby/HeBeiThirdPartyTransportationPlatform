ul.left-ico-list {
  margin: auto;
  padding-left: 0;
}

ul.left-ico-list>li:first-child {
  float: left;
  list-style: none;
}

ul.left-ico-list>li {
  float: left;
  list-style: none;
  margin-top: 2.5%;
}

.left-ico-list li .ban6 {
  background-position: -400px 0px;
}

.left-ico-list li .share {
  background-position: -300px 0px;
}

.left-ico-list li .report {
  background-position: -350px 0px;
}

.left-ico-list li .companySupervise {
  background-position: -441px -134px;
}

.left-ico-list li span {
  display: inline-block;
  width: 30px;
  height: 35px;
  background: url('../../assets/weather-icon.png') no-repeat;
  margin: 0 5px;
}

.left-ico-list li a {
  line-height: 35px;
  display: flex;
  padding: 5px 3px;
  cursor: pointer;
  color: #fff;
}

