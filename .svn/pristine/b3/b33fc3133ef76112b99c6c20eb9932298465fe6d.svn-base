  background-color: rgba(0, 0, 0, 0.6);
  color: #fff;
  border-radius: 5px;
  margin-top: 133px;
  padding-top: 20px;
  padding-bottom: 20px;
}
div.login-part>h1{
    margin: 20px auto;
}
span.loginheader-logo {
  position: absolute;
  display: inline-block;
